#include<stdio.h>
main()
{
    int n,r,sum=0,temp;
    printf("enter the value of n\n");
    scanf("%d",&n);
    temp=n;
    while(n>0)
    {
        r=n%10;
        sum=sum+(r*r*r);
        n=n/10;
    }
    if(sum==temp)
    printf("Armstrong number");
    else
    printf("Not Armstrong number");
}
